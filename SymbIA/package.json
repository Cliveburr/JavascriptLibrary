{
  "name": "symbia",
  "version": "1.0.0",
  "description": "SymbIA v2 - Multi-memory AI agent platform",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "dev:api": "pnpm --filter @symbia/api dev",
    "dev:web": "pnpm --filter @symbia/web dev",
    "dev:parallel": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "build:api": "pnpm --filter @symbia/api build",
    "build:web": "pnpm --filter @symbia/web build"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "concurrently": "^9.0.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.5.0"
  },
  "packageManager": "pnpm@9.0.0"
}